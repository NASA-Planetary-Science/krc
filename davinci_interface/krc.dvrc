			if(hasvalue(XXX.Obliq)==1){
				bodystruct.body_type="Exoplanet"                                      #### If structure has an obliquity value, the body is an exoplanet
			}
		} else {
			bodystruct=porb_type($1)
	 	}

		#if we are dealing with a planet or a moon
