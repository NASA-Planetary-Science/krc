    PD = PERIOD*86400

    #Seasonal Skin Depth in meters
    SSD1_m    = (INERTIA /(DENSITY *SPEC_HEAT ))*sqrt(PY/3.14159)
    SSD2_m    = (INERTIA2/(DENSITY2*SPEC_HEAT2))*sqrt(PY/3.14159)
