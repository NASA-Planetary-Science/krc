verbose=0

#load the default files to make things faster
krc_INERTIA=read($DV_SCRIPT_FILES+"/krc_support/ti_map2ppd_v4.vicar")
krc_ALBEDO=read($DV_SCRIPT_FILES+"/krc_support/albedo_2ppd.vicar")
